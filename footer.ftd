-- ftd.column:
width: fill-container
background.solid: #0d0d0d
padding.vw: 4
padding.vw if {ftd.device == "mobile"}: 6


-- ftd.row:
width: fill-container
spacing if {ftd.device == "desktop"}: space-between
spacing.fixed.vw if {ftd.device == "mobile"}: 8
border-top-width.px: 2
border-color: #b7ab98
padding-vertical.vw: 4
wrap: true

-- ftd.column:
width.fixed.vw: 32
width.fixed.vw if {ftd.device == "mobile"}: 48
spacing.fixed.vw: 1
spacing.fixed.vw if {ftd.device == "mobile"}: 4


-- ftd.row:
width: fill-container
spacing: space-between
wrap: true

-- ftd.image:
src: $footer.company-logo
width.fixed.vw: 6
height.fixed.vw: 6
width.fixed.vw if {ftd.device == "mobile"}: 20
height.fixed.vw if {ftd.device == "mobile"}: 16
;;border-color: red
;;border-width.px: 2


-- ftd.text: $footer.caption-1
role: $inherited.types.heading-medium 
role if {ftd.device == "mobile"}: $inherited.types.heading-small
color: #eb5939
margin-top.px if {ftd.device == "mobile"}: 10


-- end: ftd.row

-- ftd.text: $footer.about
role: $inherited.types.heading-tiny
role if {ftd.device == "mobile"}: $inherited.types.copy-large
color: #b7ab98

-- end: ftd.column

-- ftd.column:
width.fixed.vw: 20
width.fixed.vw if {ftd.device == "mobile"}: 48
spacing.fixed.vw: 2

-- ftd.text: $footer.caption-2
role: $inherited.types.heading-medium 
role if {ftd.device == "mobile"}: $inherited.types.heading-small
color: #eb5939

-- ftd.image:
